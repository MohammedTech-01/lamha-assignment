  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:specific": "react-scripts test --testPathPattern=\"(Sidebar|TransactionTable|validation)\\.test\" --coverage --collectCoverageFrom=\"src/components/Sidebar.tsx\" --collectCoverageFrom=\"src/components/TransactionTable.tsx\" --collectCoverageFrom=\"src/utils/validation.ts\"",
