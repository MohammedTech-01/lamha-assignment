    "predeploy": "npm run build",
    "docs": "npx typedoc --out docs src",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
